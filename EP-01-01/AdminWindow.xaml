<Window x:Class="EP_01_01.AdminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EP_01_01"
        mc:Ignorable="d"
        Title="AdminWindow" Height="800" Width="1280" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" Closing="Window_Closing">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="50"/>
        </Grid.RowDefinitions>

        <Frame x:Name="MainFrame" Grid.Row="0" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" NavigationUIVisibility="Hidden"/>

        <StackPanel  Grid.Row="1" Orientation="Horizontal" Width="470" VerticalAlignment="Center" HorizontalAlignment="Center">
            <Button x:Name="BtnTests" Grid.Row="1" Content="Тесты" Click="BtnTests_Click"/>
            <Button x:Name="BtnQuesitons" Grid.Row="1" Content="Вопросы" Click="BtnQuesitons_Click" Margin="10 0 0 0"/>
            <Button x:Name="BtnReports" Grid.Row="1" Content="Отчеты" Click="BtnReports_Click" Margin="10 0 0 0"/>
        </StackPanel>
        
    </Grid>
</Window>
